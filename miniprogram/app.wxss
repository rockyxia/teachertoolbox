/**app.wxss**/
.container {
  display: flex;
  flex-direction: column;
  /* align-items: center; */
  box-sizing: border-box;
  height: 100vh;
} 
.main {
  flex: 1;
  width: 100%;
  overflow-y: scroll;
}
.innerbox {
  padding: 10rpx 20rpx;
}
.footer {
  display: flex;
  justify-content: center;
  height: 150rpx;
  width: 100%;
  border-top: 1px solid #dedede;
  background-color: #fff;
}
.footer-item {
  flex: 1;
  text-align: center;
  padding-top: 10rpx;
}
.footer-nav {
  line-height: 1.2;
  background: none;
}
button.footer-nav::after{
  border: none;
}
.footer-nav .text {
  font-size: 20rpx;
}

.tr {
  text-align: right;
}
.tc {
  text-align: center;
}

/* 字体图标 */
@font-face {
    font-family: 'iconfont';
    src: url(data:font/truetype;charset=utf-8;base64,) format('truetype');
    font-weight: normal;
    font-style: normal;
}

.iconfont {
  font-family: "iconfont" !important;
  font-size: 16px;
  font-style: normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-left:before {
  content: "\e605";
}

.icon-days:before {
  content: "\e6e5";
}

.icon-jiexi:before {
  content: "\e60a";
}

.icon-fav:before {
  content: "\e60f";
}

.icon-down:before {
  content: "\e714";
}

.icon-right:before {
  content: "\e715";
}